{
  "name": "yiisoft/yii2-app-basic",
  "description": "Yii 2 Basic Project Template",
  "keywords": [
    "yii2",
    "framework",
    "basic",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yidas/yii2-bower-asset": "~2.0.5",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "kartik-v/yii2-krajee-base": "^1.8",
        "kartik-v/yii2-grid": "^3.1",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/yii2-widget-colorinput": "^1.0",
        "kartik-v/yii2-widget-touchspin": "^1.2",
        "kartik-v/yii2-widget-select2": "^2.1",
        "kartik-v/yii2-widget-typeahead": "^1.0",
        "kartik-v/yii2-widget-fileinput": "^1.0",
        "kartik-v/yii2-widget-activeform": "^1.4",
        "kartik-v/yii2-widget-depdrop": "^1.0",
        "kartik-v/yii2-detail-view": "^1.7",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-date-range": "^1.6",
        "rmrevin/yii2-fontawesome": "^3.0",
        "understeam/yii2-fcm": "^0.2.0",
        "yiisoft/yii2-authclient": "^2.1",
        "kartik-v/yii2-tabs-x": "^1.2",
        "mike42/escpos-php": "^2.0",
        "phpmailer/phpmailer": "^5.2",
        "zyx/zyx-phpmailer": "^0.9.4",
        "yii2assets/yii2-printthis": "^1.0",
        "html2text/html2text": "^4.1",
        "drewm/mailchimp-api": "^2.5",
        "kartik-v/yii2-widget-alert": "^1.1",
        "chd7well/yii2-wizard": "^0.0.4",
        "chrisb34/yii2-report-grid": "dev-master",
        "reportico/yii2-reportico": "^4.6"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
